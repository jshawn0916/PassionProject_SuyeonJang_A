@model IEnumerable<PassionProject_SuyeonJang_A.Models.DestinationDto>
@{
    int index = 1;
}
@{ ViewBag.Title = "List"; }

<h2>Journey List</h2>

<div>

    <table id="journeyTable">
        <tbody>
            @foreach (PassionProject_SuyeonJang_A.Models.DestinationDto Destination in Model)
            {
                <tr>
                    <td><b>@index</b></td>
                    <td>@Destination.DestinationName</td>
                    <td><button>수정</button></td>
                    <td><button>삭제</button></td>
                </tr>
                index++;
            }
        </tbody>
    </table>
</div>
